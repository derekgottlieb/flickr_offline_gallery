<html>
  <head>
    <style>
      body { background-color: dimgrey; }
      img {
        height: 200px;
        padding: 0px;
        margin: 0px;
        border: 1px solid black;
      }
    </style>
  </head>
<body>
<h1><%= photoset.title %> by <%= photoset.username %></h1>
<% photos.each do |photo| %>
